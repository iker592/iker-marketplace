      "Bash(df:*)",
      "Bash(echo:*)",
      "Bash(jq:*)",
      "Bash(date)",
      "Bash(uname:*)",
      "Bash(whoami)",
      "Bash(hostname)",

      "Bash(git status:*)",
      "Bash(git status)",
      "Bash(git log:*)",
      "Bash(git diff:*)",
      "Bash(git branch:*)",
      "Bash(git fetch:*)",
      "Bash(git show:*)",
